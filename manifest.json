{
    "name": "mixSearch",
    "description" : "mixSearch",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "mixSearch.png"
    },
    "icons": {
        "16": "icon16.jpg"
    },
    "permissions": [
        "history",
        "contextMenus",
        "management",
        "system.cpu",
        "system.memory",
        "system.storage"
    ],
    "chrome_url_overrides" : {
        "newtab": "newTab.html"
    },
    "manifest_version": 2,
    "background": {
        "scripts": [
          "background.js"
        ]
    }
}